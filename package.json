{
  "name": "nestjs-starter",
  "version": "1.0.0",
  "author": "hmake98",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "postinstall": "npx typesync && yarn generate",
    "build": "nest build",
    "dev": "dotenv -e .env.local -- nest start --watch",
    "debug": "nest start --debug --watch",
    "generate": "prisma generate",
    "start": "node dist/main",
    "migrate": "dotenv -e .env.local -- prisma migrate dev",
    "migrate:prod": "prisma migrate deploy",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts'",
    "test": "dotenv -e .env.local -- jest --runInBand --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json --forceExit",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.554.0",
    "@aws-sdk/client-ses": "^3.554.0",
    "@aws-sdk/client-sns": "^3.554.0",
    "@aws-sdk/s3-request-presigner": "^3.554.0",
    "@faker-js/faker": "^8.4.1",
    "@nestjs-modules/mailer": "^1.11.2",
    "@nestjs/bull": "^10.1.1",
    "@nestjs/common": "^10.3.7",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.7",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.7",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^5.1.2",
    "aws-sdk-client-mock": "^4.0.0",
    "bcrypt": "5.1.0",
    "bull": "^4.12.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express-rate-limit": "^7.2.0",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jest-junit": "^16.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "nestjs-console": "^9.0.0",
    "nestjs-i18n": "^10.4.5",
    "nodemailer": "^6.9.13",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma": "^5.12.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/testing": "^10.3.7",
    "@prisma/client": "^5.12.1",
    "@types/bcrypt": "^10.0.28-alpha",
    "@types/bull": "^4.10.0",
    "@types/copyfiles": "^2.4.4",
    "@types/eslint": "~8.56.9",
    "@types/eslint-config-prettier": "~6.11.3",
    "@types/i18n": "^0.13.10",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lint-staged": "^13.3.0",
    "@types/lodash": "^4.17.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.7",
    "@types/nodemailer": "^6.4.14",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.11.5",
    "@types/prettier": "^2.7.3",
    "@types/pretty-quick": "^3.1.6",
    "@types/rimraf": "^3.0.2",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.0.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.4.5"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "endOfLine": "auto"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": ".",
          "outputName": "junit.xml"
        }
      ]
    ]
  },
  "lint-staged": {
    "*.{js,ts}": "yarn lint:fix",
    "*.{ts,js,json,md}": "prettier --write"
  }
}
