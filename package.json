{
  "name": "nestjs-starter",
  "version": "1.0.0",
  "description": "A NestJS starter project with comprehensive setup",
  "author": "hmake98",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "node dist/main",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "update:package": "npx npm-check-updates -u",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev",
    "migrate:prod": "prisma migrate deploy",
    "studio": "prisma studio",
    "seed:email": "nestjs-command seed:emails",
    "rollback:email": "nestjs-command rollback:emails",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "jest --config test/jest.json --runInBand --passWithNoTests --forceExit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config test/jest.json --runInBand"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.635.0",
    "@aws-sdk/client-ses": "^3.636.0",
    "@aws-sdk/client-sns": "^3.635.0",
    "@aws-sdk/s3-request-presigner": "^3.635.0",
    "@faker-js/faker": "^8.4.1",
    "@nestjs/bull": "^10.2.0",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.4.1",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.1",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^6.1.1",
    "@prisma/client": "^5.18.0",
    "@sentry/node": "^8.49.0",
    "@sentry/tracing": "^7.114.0",
    "@types/express": "^4.17.21",
    "argon2": "^0.40.3",
    "bull": "^4.16.0",
    "cache-manager": "^5.7.6",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "nestjs-command": "^3.1.4",
    "nestjs-i18n": "^10.4.5",
    "nestjs-pino": "^4.4.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino": "^9.5.0",
    "pino-http": "^10.4.0",
    "pino-pretty": "^11.3.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@nestjs/cli": "^10.4.4",
    "@nestjs/testing": "^10.4.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.13",
    "@swc/jest": "^0.2.37",
    "@types/bull": "^4.10.0",
    "@types/compression": "^1.7.5",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.5.0",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.1.0",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "prisma": "^5.18.0",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.5.4"
  },
  "lint-staged": {
    "{src,test}/**/*.{ts,js,json}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
