{
  "name": "nestjs-starter",
  "version": "0.0.1",
  "author": "hmake98",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "nest build",
    "start": "node dist/main",
    "dev": "dotenv -e .env.local -- nest start --watch",
    "console:dev": "dotenv -e .env.development -- ts-node -r tsconfig-paths/register src/console.ts",
    "console:test": "dotenv -e .env.test -- ts-node -r tsconfig-paths/register src/console.ts",
    "console:prod": "ts-node -r tsconfig-paths/register src/console.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "jest",
    "test:local": "dotenv -e .env.test -- jest",
    "test:local-e2e": "dotenv -e .env.test -- jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "engines": {
    "node": "14.16.0"
  },
  "dependencies": {
    "@nestjs/bull": "^0.4.2",
    "@nestjs/common": "^9.0.3",
    "@nestjs/core": "^9.0.3",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.3",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/swagger": "^6.0.4",
    "@nestjs/terminus": "^9.0.0",
    "@nestjs/throttler": "^3.1.0",
    "@nestjs/typeorm": "^9.0.1",
    "aws-sdk": "^2.942.0",
    "bcrypt": "^5.0.1",
    "bull": "^3.29.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "date-fns": "^2.21.3",
    "dotenv": "^9.0.2",
    "express-rate-limit": "^6.7.0",
    "firebase-admin": "^11.0.0",
    "helmet": "^4.6.0",
    "ioredis": "^4.28.5",
    "jest-junit": "^15.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "nanoid": "^3.1.23",
    "nestjs-console": "^8.0.0",
    "nestjs-i18n": "^9.1.5",
    "nestjs-pino": "^3.1.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.4.0",
    "pino-http": "^8.3.3",
    "pino-pretty": "^9.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "0.3.11",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.0",
    "@commitlint/config-conventional": "^17.4.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^9.0.3",
    "@types/bcrypt": "^10.0.28-alpha",
    "@types/bull": "^3.15.7",
    "@types/copyfiles": "^2.4.0",
    "@types/eslint": "^7.2.10",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/i18n": "^0.13.1",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^28.1.8",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.169",
    "@types/morgan": "^1.9.4",
    "@types/node": "^16.11.10",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/pg": "^8.6.6",
    "@types/prettier": "^2.2.3",
    "@types/rimraf": "^3.0.0",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^12.3.4",
    "prettier": "^2.8.1",
    "pretty-quick": "^3.1.3",
    "sequelize-cli": "^6.5.2",
    "supertest": "^6.3.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "10.7.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "4.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": ".",
          "outputName": "junit.xml"
        }
      ]
    ]
  },
  "lint-staged": {
    "*.{js,ts}": "npm run lint:fix",
    "*.{ts,js,json,md}": "prettier --write"
  }
}
