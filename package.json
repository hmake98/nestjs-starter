{
  "name": "nestjs-starter",
  "version": "0.0.1",
  "description": "NestJS starter pack with TypeScript, Postgres with Prisma, Swagger, JWT, and more",
  "author": "hmake98",
  "private": true,
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "prebuild": "rimraf build",
    "postinstall": "npx typesync",
    "build": "nest build && copyfiles Procfile build/",
    "stage": "cross-env NODE_ENV=stage node build/main",
    "prod": "cross-env NODE_ENV=production node build/main",
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "prisma": "prisma",
    "pre-commit": "pretty-quick --staged",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install"
  },
  "engines": {
    "node": "16.16.0"
  },
  "dependencies": {
    "@nestjs/bull": "^0.4.2",
    "@nestjs/common": "^9.0.3",
    "@nestjs/core": "^9.0.3",
    "@nestjs/platform-express": "^9.0.3",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/swagger": "^6.0.4",
    "@nestjs/terminus": "^9.0.0",
    "@prisma/client": "^4.0.0",
    "aws-sdk": "^2.942.0",
    "bcrypt": "^5.0.1",
    "bull": "^3.29.3",
    "bunyan": "^1.8.15",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "date-fns": "^2.21.3",
    "dotenv": "^9.0.2",
    "express-session": "^1.17.2",
    "firebase-admin": "^11.0.0",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "morgan-json": "^1.1.0",
    "nanoid": "^3.1.23",
    "nestjs-i18n": "^9.1.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^9.0.3",
    "@types/bcrypt": "^10.0.28-alpha",
    "@types/bull": "^3.15.7",
    "@types/bunyan": "^1.8.6",
    "@types/copyfiles": "^2.4.0",
    "@types/eslint": "^7.2.10",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/express-session": "^1.17.3",
    "@types/i18n": "^0.13.1",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.169",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.18.21",
    "@types/prettier": "^2.2.3",
    "@types/rimraf": "^3.0.0",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^12.3.4",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "prisma": "^4.0.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
